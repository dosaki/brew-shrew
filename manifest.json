{
  "manifest_version": 2,

  "name": "Brew Shrew",
  "description": "Ask for your brew",
  "version": "0.2",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "icons":{
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "cookies",
    "http://*/"
  ]
}
