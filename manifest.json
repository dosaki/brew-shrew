{
  "manifest_version": 2,

  "name": "Brew Shrew",
  "description": "Ask for your brew",
  "version": "0.1",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
    "cookies",
    "http://*/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  }
}
